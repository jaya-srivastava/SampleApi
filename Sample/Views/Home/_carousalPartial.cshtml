                    <br />
                Become a Member Today!</p>                
                @Html.ActionLink("PARENT", "register", "membership", new { id =  "parent" }, new { @class = "btn-white margin-right20 btn-white-blue" })
                @Html.ActionLink("TEACHER", "register", "membership", new { id =  "teacher" }, new { @class = "btn-white margin-right20 btn-white-sea" })
            </div>
        </div>
        <div class="item jumbotron home-backcolor-blue home-carousel-height">
