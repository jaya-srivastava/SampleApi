<div id="myCarousel" class="carousel">
    <div class="carousel-inner carousal-main-height">
        <div class="item jumbotron home-backcolor-green active home-carousel-height">
            <div class="container margin45">
                <h3 class="text-shadow-green"><i class="fa fa-check-square-o"></i>&nbsp;High School Math</h3><br />
                <p>Algebra - Matrices and Quadratics.</p>
                <p>Differential Calculus and Integral Calculus</p>
                <br />
                @Html.ActionLink("Practice Calculus", "grade-10-skills", "grade", null, new { @class = "btn-white btn-white-orange" })
            </div>
        </div>
        <div class="item home-backcolor-orange jumbotron home-carousel-height">
            <div class="container margin45">
