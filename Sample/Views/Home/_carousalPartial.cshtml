                <br />                
                @Html.ActionLink("PRACTICE A MATH SKILL", "index", "Topic", null, new { @class = "btn-white btn-white-blue" })
            </div>
        </div>
        <div class="item home-backcolor-green jumbotron home-carousel-height">
